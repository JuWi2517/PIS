	  <h1 style="text-align: center; margin: 3%;">Editovat Materiál</h1>
	  <div style="display:grid;margin:25px;">
	   <input type="text" id="materialId" style="display:none;">
		<label for="mName">Název Materiálu:</label><input type="text" id="mName" name="mName"><br>
		<label for="mDescription">Popis materiálu:</label><input type="text" id="mDescription" name="mDescription"><br>
		<label for="sPrice">Cena za jednotku:</label><input type="number" id="sPrice" name="sPrice"><br>
		<label for="sUnit">Měrná jednotka:</label>
		<select id="sUnit" name="sUnit">
			<option value="kg">kg<option>
			<option value="t">t<option>
			<option value="ks">ks<option>
			<option value="ks">palety<option>
		</select>
		<label for="mCountStock">Počet Kusů:</label><input type="number" id="mCountStock" name="mCountStock"><br>
		<label for="sSupplier">Dodavatel:</label><select id="sSupplier" name="sSupplier"></select><br>
		
		
		<input type="submit" class="btn btn-primary" id = "addM" value="Editovat" onclick="updateMaterial()">
		</div>
	
		
		
			
	
				
		
	
		

		<script>
			const queryString = window.location.search;		
			const urlParams = new URLSearchParams(queryString);
			const product = urlParams.get('id')
			getMaterialById("zkouska",product);
			
			var idSupplierHid = document.getElementById('materialId');	
			idSupplierHid.value = product;

			getAllSuppliersSelect('sSupplier');
		
		
		
		</script>
		
	  		 
			  
		  
		
